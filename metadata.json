      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "6", "7" ]
    }
  ],
  "dependencies": [
    { "name": "puppetlabs-stdlib", "version_requirement": ">=4.10.0 < 5.0.0" },
    { "name": "puppetlabs-apt", "version_requirement": ">= 2.0.0 < 3.0.0" }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": "> = 3.0.0 < 5.0.0" }
  ]
